{
    "manifest_version": 2,
    "name": "test config from ",
    "version": "0.1",
    "options_ui": {
        "page": "options.html",
        "browser_style": true,
        "open_in_tab": false
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "<all_urls>",
        "webRequest",
        "storage",
        "activeTab",
        "history",
        "tabs"
    ],
    "web_accessible_resources": [
        "images/kodi-logo.png"
    ],
    "content_scripts": [{
        "matches": [
            "https://www.youtube.com/watch?v=*"
        ],
        "js": [
            "axios.min.js",
            "content.js"
        ],
        "run_at": "document_end"
    }]
}